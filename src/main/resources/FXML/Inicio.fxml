<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #4287FF;"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ui.pantallas.pantallainicio.InicioController">
    <ImageView fx:id="gdlogo" fitHeight="173.0" fitWidth="507.0" layoutX="147.0" layoutY="42.0" pickOnBounds="true"
               preserveRatio="true"/>
    <ImageView fx:id="playbutton" fitHeight="150.0" fitWidth="206.0" layoutX="297.0" layoutY="225.0"
               onMouseClicked="#cargarPantallaBusqueda" pickOnBounds="true" preserveRatio="true"/>
    <ImageView fx:id="credits" fitHeight="102.0" fitWidth="109.0" layoutX="654.0" layoutY="476.0" pickOnBounds="true"
               preserveRatio="true" onMouseClicked="#mostrarCreditos"/>
</AnchorPane>
